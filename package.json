{
  "name": "react-redux-universal-hot-example",
  "description": "Example of an isomorphic (universal) webapp using react redux and hot reloading",
  "author": "Erik Rasmussen <rasmussenerik@gmail.com> (http://github.com/erikras)",
  "license": "MIT",
  "version": "0.0.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/erikras/react-redux-universal-hot-example"
  },
  "homepage": "https://github.com/erikras/react-redux-universal-hot-example",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start": "node ./node_modules/better-npm-run start",
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "lint": "eslint -c .eslintrc src",
    "start-dev": "node ./node_modules/better-npm-run start-dev",
    "watch-client": "node ./node_modules/better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "test": "karma start"
  },
  "betterScripts": {
    "start": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development"
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
  "dependencies": {
    "babel": "*",
    "babel-plugin-typecheck": "*",
    "body-parser": "*",
    "compression": "*",
    "express": "*",
    "express-session": "*",
    "file-loader": "*",
    "http-proxy": "*",
    "lru-memoize": "*",
    "map-props": "*",
    "piping": "*",
    "pretty-error": "*",
    "query-string": "*",
    "react": "*",
    "react-document-meta": "*",
    "react-inline-css": "*",
    "react-redux": "*",
    "react-router": "^1.0.0-beta3",
    "redux": "*",
    "redux-form": "*",
    "serialize-javascript": "*",
    "serve-favicon": "*",
    "serve-static": "*",
    "superagent": "*",
    "url-loader": "*",
    "webpack-isomorphic-tools": "*"
  },
  "devDependencies": {
    "autoprefixer-loader": "^*",
    "babel-core": "*",
    "babel-eslint": "*",
    "babel-loader": "*",
    "babel-runtime": "*",
    "better-npm-run": "*",
    "chai": "*",
    "clean-webpack-plugin": "*",
    "concurrently": "*",
    "css-loader": "*",
    "eslint": "*",
    "eslint-config-airbnb": "*",
    "eslint-plugin-react": "*",
    "extract-text-webpack-plugin": "*",
    "json-loader": "*",
    "karma": "*",
    "karma-chrome-launcher": "*",
    "karma-cli": "*",
    "karma-firefox-launcher": "*",
    "karma-mocha": "*",
    "karma-mocha-reporter": "*",
    "karma-sourcemap-loader": "*",
    "karma-webpack": "*",
    "mocha": "*",
    "node-sass": "^3.3.2",
    "react-a11y": "*",
    "react-hot-loader": "*",
    "redux-devtools": "*",
    "sass-loader": "*",
    "strip-loader": "*",
    "style-loader": "*",
    "webpack": "*",
    "webpack-dev-server": "*"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}
